<Window x:Class="InternetMarket.Windows.Contragent"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InternetMarket.Windows"
        mc:Ignorable="d"
        Title="Contragent" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <TextBox Name="contragentname" Grid.Row="0" Margin="10"/>
        <ComboBox Name="countrycombo" Grid.Row="1" Margin="10"/>
        <ComboBox Name="citycombo" Grid.Row="2" Margin="10"/>
        <ComboBox Name="oblastcombo" Grid.Row="3" Margin="10"/>
        <ComboBox Name="regioncombo" Grid.Row="4" Margin="10"/>
        <ComboBox Name="steetcombo" Grid.Row="5" Margin="10"/>
        <Button Name="btnSet" Content="Set" Grid.Row="6" Margin="10" Click="BtnSet_Click" />
    </Grid>
</Window>
