<Window x:Class="InternetMarket.Windows.Spravochniki.SkladAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InternetMarket.Windows.Spravochniki"
        mc:Ignorable="d"
        Title="SkladAdd" Height="450" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <TextBox Name="nametext" Grid.Row="0"  Margin="15"/>
        <TextBox Name="typetext" Grid.Row="1"  Margin="15"/>
        <ComboBox Name="organisationtext" Grid.Row="2"  Margin="15"/>
        <ComboBox Name="oblasttext" Grid.Row="3"  Margin="15"/>
        <ComboBox Name="citytext" Grid.Row="4"  Margin="15"/>
        <ComboBox Name="streettext" Grid.Row="5"  Margin="15"/>
        <Button Name="setBtn" Grid.Row="6"  Margin="15" Content="Загрузить" Click="SetBtn_Click"/>

    </Grid>
</Window>
