<Window x:Class="InternetMarketClient.Windows.Country"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InternetMarketClient.Windows"
        mc:Ignorable="d"
        Title="Country" Height="450" Width="800">
    <Grid>

        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <TextBox Name="CountryName" Grid.Row="0" Margin="20"/>
        <Button Grid.Row="11" Name="btnSet" Margin="30" Click="BtnSet_Click" />
    </Grid>
</Window>
