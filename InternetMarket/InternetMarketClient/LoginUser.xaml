<Window x:Class="InternetMarketClient.LoginUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InternetMarketClient"
        mc:Ignorable="d"
        Title="LoginUser" Height="300" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBox Name="Name" Grid.Row="0" Margin="20"/>
        <PasswordBox Name="Password" Grid.Row="1" Margin="20"/>
        <Button Name="btlLogin" Grid.Row="2" Margin="20" Content="Логин" Click="BtlLogin_Click"/>
    </Grid>
</Window>
